train_net: "models/VGG16/mnc_5stage/train_instanceSeg.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 20000
display: 10
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 1000
snapshot_prefix: "vgg16_mnc_instanceSeg"
iter_size: 8
